package com.chainsys.database;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.ResultSet;
import java.sql.SQLException;
import java.sql.Statement;
  
public class Student {

	public static void main(String[] args) throws SQLException {
		// TODO Auto-generated method stub
String query="select doctor from hospital";
		Connection con= DriverManager.getConnection("jdbc:orcale://thin@localhost:1521/poo","system","Rpoo5397");
	Statement st=con.createStatement();
	ResultSet rs=st.executeQuery(query);
	while(rs.next())
	{
		System.out.println(rs.getString("doctor"));
	}
	st.close();
	con.close();
	}

}
